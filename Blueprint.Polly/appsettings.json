{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "AllowedHosts": "*",
  "FaultHandling": {
    "MaxRetryAttempts": 2,
    "InitialRetryDelayInMs": 10,
    "ExceptionsAllowedBeforeBreaking": 2,
    "DurationOnBreakInSec": 30,
    "WebApiTimeoutInMs": 1800
  },
  "ApplicationSettings": {
    "ProductionEnvironment": "Production",
    "WeatherForecastApi": {
      "ProviderName": "WeatherForecast",
      "RequestUri": "https://localhost:44312"
    }
  }
}
